<xs:schema xmlns="https://github.com/Region-Skane-SDI/diml/dataproduct" targetNamespace="https://github.com/Region-Skane-SDI/diml/dataproduct" xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified">
  <xs:element name="dataProduct" type="dataProduct" />
  <xs:complexType name="dataProduct">
    <xs:annotation>
      <xs:documentation>Beskrivning av en dataprodukt med dess specifikationer.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="names" type="names" minOccurs="1" maxOccurs="1" />
      <xs:element name="descriptions" type="descriptions" minOccurs="1" maxOccurs="1" />
      <xs:element name="dcat" type="dcat" minOccurs="1" maxOccurs="1" />
      <xs:element name="traits" type="traits" minOccurs="0" maxOccurs="1" />
      <xs:element name="dataInputs" type="dataInputs" minOccurs="1" maxOccurs="1" />
      <xs:element name="tables" type="tables" minOccurs="0" maxOccurs="1" />
      <xs:element name="customFormats" type="customFormats" minOccurs="0" maxOccurs="1" />
      <xs:element name="dataOutputs" type="dataOutputs" minOccurs="0" maxOccurs="1" />
      <xs:element name="dataFlow" type="dataFlow" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="dimlId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>En globalt unik identifierare för Diml-objektet.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="targetPlatform" type="targetPlatformType" use="required">
      <xs:annotation>
        <xs:documentation>Anger vilken plattform som ska användas.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="uniqueName" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Ett globalt unikt namn f�r en dataspecifikation.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="dataFlow">
    <xs:annotation>
      <xs:documentation>Anger flödet av data genom alla lager, samt egenskaper t.ex om en tabell ska materialiseras, uppdateras osv.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="dataStages" type="dataStages" minOccurs="1" maxOccurs="1" />
      <xs:element name="dataIngests" type="dataIngests" minOccurs="1" maxOccurs="1" />
    </xs:all>
  </xs:complexType>
  <xs:complexType name="dataStages">
    <xs:sequence>
      <xs:element name="dataStage" type="dataStage" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="dataStage">
    <xs:annotation>
      <xs:documentation>Anger de olika stadien för data och dess egenskaper, tex. Om en tabell ska materialiseras eller inte, hur data ska uppdateras osv.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="traits" type="traits" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="id" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Unikt id för dataStage.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="materialization" type="dataStageMaterialization" use="required">
      <xs:annotation>
        <xs:documentation>Anger om data ska materialiseras eller virtualiseras.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="order" type="xs:int" use="required">
      <xs:annotation>
        <xs:documentation>Anger ordningen på dataStage, dvs vilket steg som ska ske först.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="role" type="dataStageRole" use="optional">
      <xs:annotation>
        <xs:documentation>Anger vad som sker i dataStage.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="type" type="dataStageType" use="required">
      <xs:annotation>
        <xs:documentation>Anger vilken typ av dataStage .</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="dataIngests">
    <xs:sequence>
      <xs:element name="dataIngest" type="dataIngest" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="dataIngest">
    <xs:annotation>
      <xs:documentation>Anger hur data ska inhämtas från källa till destination.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="tableToTableMappings" type="tableToTableMappings" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="customSqlExpression" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>TODO</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dataInputId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Ett unikt id som respesenterar en källa.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="id" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Ett unikt id som representerar ett dataIngest-flöde.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="tableToTableMappings">
    <xs:sequence>
      <xs:element name="tableToTableMapping" type="tableToTableMapping" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="tableToTableMapping">
    <xs:annotation>
      <xs:documentation>TODO</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="columnMappings" type="columnMappings" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="allowSchemaDrift" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>TODO</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="customSqlExpression" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>TODO</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="sourceTableId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>TODO</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="tableId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>TODO</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="columnMappings">
    <xs:sequence>
      <xs:element name="columnMapping" type="columnMapping" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="columnMapping">
    <xs:annotation>
      <xs:documentation>TODO</xs:documentation>
    </xs:annotation>
    <xs:attribute name="columnId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>TODO</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="sourceColumnId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>TODO</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="dcat">
    <xs:annotation>
      <xs:documentation>Innehåller egenskaper för DCAT vilket är en metadataspecifikation för att beskriva datamängder. Mer information finns här: https://docs.dataportal.se/dcat/en/</xs:documentation>
    </xs:annotation>
    <xs:attribute name="dcatAccessRights" type="dcatAccessRightsType" use="required">
      <xs:annotation>
        <xs:documentation>Anger vilka åtkomsträttigheter som finns för dataprodukten i datakatalogen. Följer standarden DCAT-AP, mer information finns här: https://docs.dataportal.se/dcat/en/#5.5</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dcatAvailability" type="dcatAvailabilityType" use="required">
      <xs:annotation>
        <xs:documentation>Anger dataproduktens tillgänglighet i datakatalogen. Följer standarden DCAT-AP, mer information finns här: https://docs.dataportal.se/dcat/en/#5.9</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dcatDataTheme" type="dcatDataThemeType" use="required">
      <xs:annotation>
        <xs:documentation>Anger vilket datatema dataprodukten tillhör i datakatalogen. Följer standarden DCAT-AP, mer information finns här: https://docs.dataportal.se/dcat/en/#5.2</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dcatStatus" type="dcatStatusType" use="required">
      <xs:annotation>
        <xs:documentation>Anger dataproduktens status i datakatalogen. Följer standarden DCAT-AP, mer information finns här: https://docs.dataportal.se/dcat/en/#5.8</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dcatUpdateFrequency" type="dcatUpdateFrequencyType" use="required">
      <xs:annotation>
        <xs:documentation>Anger dataproduktens uppdateringsfrekvens i datakatalogen. Följer standarden DCAT-AP, mer information finns här: https://docs.dataportal.se/dcat/en/#5.4</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="dataInputs">
    <xs:sequence>
      <xs:element name="dataInput" type="dataInput" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="dataInput">
    <xs:annotation>
      <xs:documentation>Anger en indatakälla för en dataproduktsspecifikation, t.ex. en DataSource eller en annan DataProduct.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="channelId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Anger vilken typ av data som ska hämtas och i vilket format.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="dimlId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Ett globalt unikt dimlId för indatakällan.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="id" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Ett id för indatakällan. Detta måste vara unikt inom en DataProduct då det kommer att användas som ett alias.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="tables">
    <xs:sequence>
      <xs:element name="table" type="table" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="table">
    <xs:annotation>
      <xs:documentation>Beskriver en tabell i en tabulär dataprodukt.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="names" type="names" minOccurs="1" maxOccurs="1" />
      <xs:element name="descriptions" type="descriptions" minOccurs="0" maxOccurs="1" />
      <xs:element name="traits" type="traits" minOccurs="0" maxOccurs="1" />
      <xs:element name="businessKey" type="businessKey" minOccurs="0" maxOccurs="1" />
      <xs:element name="tableInstances" type="tableInstances" minOccurs="0" maxOccurs="1" />
      <xs:element name="commonColumns" type="commonColumns" minOccurs="0" maxOccurs="1" />
      <xs:element name="columns" type="columns" minOccurs="1" maxOccurs="1" />
      <xs:element name="tableRelations" type="tableRelations" minOccurs="0" maxOccurs="1" />
      <xs:element name="sampleData" type="sampleData" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="id" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Tabellens id.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="tableInstances">
    <xs:annotation>
      <xs:documentation>En samling av tabellinstanser i en tabulär dataprodukt.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="tableInstance" type="tableInstance" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="existsFromStageId" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Anger från och med vilket dataStage som tabellen kommer att existera i. Om inget anges gäller dataStage med order="1".</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="existsToStageId" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Anger till och med vilket dataStage som tabellen kommer att existera i. Om inget anges gäller dataStage med order="X", där X = högsta order.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="tableInstance">
    <xs:annotation>
      <xs:documentation>Anger en instans av en tabell i en tabulär dataprodukt.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="alias" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Anger tabellens alias för ett eller flera dataStages.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fromStageId" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Anger från och med vilket dataStage som tabellen ska byta namn i. Om inget anges gäller dataStage med order="1".</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="toStageId" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Anger till och med vilket dataStage som tabellen ska byta namn i. Om inget anges gäller dataStage med order="X", där X = högsta order.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="businessKey">
    <xs:sequence>
      <xs:element name="value" type="businessKeyValue" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="businessKeyValue">
    <xs:restriction base="xs:string" />
  </xs:simpleType>
  <xs:complexType name="commonColumns">
    <xs:sequence>
      <xs:element name="commonColumn" type="commonColumn" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="commonColumn">
    <xs:annotation>
      <xs:documentation>Beskriver en referens till en återanvändbar kolumn.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="columnInstances" type="columnInstances" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="id" type="commonColumnIdType" use="required">
      <xs:annotation>
        <xs:documentation>Kolumnens id.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="columns">
    <xs:sequence>
      <xs:element name="column" type="column" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="column">
    <xs:annotation>
      <xs:documentation>Beskriver en kolumn i en tabulär dataprodukt.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="names" type="names" minOccurs="1" maxOccurs="1" />
      <xs:element name="dimlDataType" type="dimlDataType" minOccurs="1" maxOccurs="1" />
      <xs:element name="columnInstances" type="columnInstances" minOccurs="0" maxOccurs="1" />
      <xs:element name="descriptions" type="descriptions" minOccurs="0" maxOccurs="1" />
      <xs:element name="traits" type="traits" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="id" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Kolumnens id.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="columnInstances">
    <xs:annotation>
      <xs:documentation>En samling av kolumninstanser i en tabulär dataprodukt.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="columnInstance" type="columnInstance" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="existsFromStageId" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Anger från och med vilket dataStage som kolumnen ska existera i. Om inget anges gäller dataStage med order="1".</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="existsToStageId" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Anger till och med vilket dataStage som kolumnen ska existera i. Om inget anges gäller dataStage med order="X", där X = högsta order.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="columnInstance">
    <xs:annotation>
      <xs:documentation>Anger en instans av en kolumn i en tabulär dataprodukt.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="dimlDataType" type="dimlDataType" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="alias" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Anger kolumnens alias för ett eller flera dataStages.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="fromStageId" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Anger från och med vilket dataStage som kolumnen ska byta namn och/eller datatyp i. Om inget anges gäller dataStage med order="1".</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="toStageId" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Anger till och med vilket dataStage som kolumnen ska byta namn och/eller datatyp i. Om inget anges gäller dataStage med order="X", där X = högsta order.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="dimlDataType">
    <xs:attribute name="id" type="dimlDataTypeConfigType" use="required">
      <xs:annotation>
        <xs:documentation>Anger vilken Diml-datatyp som ska användas, baserat på ett ID från en
                    dataTypeAttribute i dataTypes &gt; dataTypeAttributes.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="length" type="xs:int" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="format" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="precision" type="xs:int" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="scale" type="xs:int" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="isFixedLength" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="isWideChar" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="unsigned" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="originalDataType" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="originalPlatform" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="isCaseSensitive" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="isNullable" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="isUtcTime" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>En implementation av ett attribut i dataTypes &gt; dataTypeAttributes med samma ID.
                    </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="tableRelations">
    <xs:sequence>
      <xs:element name="tableRelation" type="tableRelation" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="tableRelation">
    <xs:annotation>
      <xs:documentation>Anger eventuella tabellrelationer och hur de relaterar via relationsColumns.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="relationColumns" type="relationColumns" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="id" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Ett unikt id för tabellrelationen.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="targetTable" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Anger vilken tabell som tabellrelationen relaterar till.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="relationColumns">
    <xs:annotation>
      <xs:documentation>Anger med vilka kolumner en tabell relaterar till en annan.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="columnName" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Anger relationkolumnens namn i källtabellen.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="targetColumnName" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Anger relationkolumnens namn i den relaterade tabellen.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="sampleData">
    <xs:sequence>
      <xs:element name="sampleDataRow" type="sampleDataRow" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="sampleDataRow">
    <xs:annotation>
      <xs:documentation>Anger exempeldata.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="value" type="stringValueType" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="customFormats">
    <xs:sequence>
      <xs:element name="customFormat" type="customFormat" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="customFormat">
    <xs:annotation>
      <xs:documentation>Beskriver ett egendefinierat utdataformat.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="customFormatId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Ett id för det egendefinierade udataformatet. Id:et måste vara unikt inom en dataproduktspecifikation då det kommer att användas som ett alias.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="formatType" type="formatType" use="required">
